\begin_layout Standard
נניח בדרך השלילה שאין נביעה לוגית קיים מבנה 
\begin_inset Formula $M$
\end_inset

 והשמה 
\begin_inset Formula $s$
\end_inset

 כך ש-
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
 & M\underset{s}{\vDash}\forall x(\alpha\rightarrow\beta)\\
 & M\underset{s}{\nvDash}\forall x\alpha\rightarrow\forall x\beta\\
 & \Updownarrow\\
 & M\underset{s}{\vDash}\forall x\alpha\\
 & M\not\underset{s}{\vDash}\forall x\beta\\
 & \Updownarrow\\
 & \exists d_{0}\in D^{M}\\
 & M\underset{s[x\leftarrow d_{0}]}{\vDash}\beta\\
 & M\underset{s}{\vDash}\forall x\alpha\\
 & \Updownarrow\\
 & \forall d\in D^{M}\\
 & M\underset{s[x\leftarrow d]}{\vDash}\alpha\\
 & M\underset{s[x\leftarrow d_{0}]}{\vDash}\alpha\\
 & M\underset{s[x\leftarrow d_{0}]}{\nvDash}\alpha\\
 & \Downarrow\\
 & M\underset{s}{\nvDash}\forall x(\alpha\rightarrow\beta)
\end{align*}

\end_inset


\end_layout

