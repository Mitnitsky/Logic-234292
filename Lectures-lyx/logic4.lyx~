\begin_layout Standard
מגדירים 
\begin_inset Formula $\overline{V}=WFF\rightarrow\{T,F\}$
\end_inset


\end_layout

\begin_layout Standard
אם 
\begin_inset Formula $\alpha=p_{i}$
\end_inset

 
\begin_inset Formula $\overline{V}(p_{i})=v(p_{i})$
\end_inset


\end_layout

\begin_layout Standard
אם 
\begin_inset Formula $\alpha=(\beta\square\gamma)$
\end_inset

 אז 
\begin_inset Formula $\overline{V}(\alpha)=TT_{\square}(\overline{V}(\beta),\overline{V}(\gamma))$
\end_inset


\end_layout

\begin_layout Standard
אם 
\begin_inset Formula $\alpha=(\neg\beta)$
\end_inset

 אז 
\begin_inset Formula $\overline{V}(\alpha)=TT_{\neg}(\overline{V}(\beta))$
\end_inset


\end_layout

