\begin_layout Subsubsection*
דוגמה:
\end_layout

\begin_layout Standard
\begin_inset Formula $\alpha\models\beta$
\end_inset

, 
\begin_inset Formula $\alpha\rightarrow\beta$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\{\alpha\rightarrow\beta,\alpha\}\models\beta$
\end_inset


\end_layout

\begin_layout Subsubsection*
למה:
\end_layout

\begin_layout Standard
\begin_inset Formula $X,\alpha\models\beta$
\end_inset

 אם ורק אם 
\begin_inset Formula $X\models\alpha\rightarrow\beta$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\text{\fbox{X\cup\{\alpha\}\models\beta}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
 & \alpha_{1},\alpha_{2},a_{3}\models\beta\\
 & \alpha_{1},\alpha_{2}\models(\alpha_{3}\rightarrow\beta)\\
 & \alpha_{1}\models(\alpha_{2}\rightarrow(\alpha_{3}\rightarrow\beta))\\
 & \models(\alpha_{1}\rightarrow(\alpha_{2}\rightarrow(\alpha_{3}\rightarrow\beta)))
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection*
סימון:
\end_layout

\begin_layout Standard
\begin_inset Formula $M(\alpha)\{v|v\models\alpha\}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $M(X)=\{v|v\models X\}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\alpha$
\end_inset

 סתירה: 
\begin_inset Formula $M(\alpha)=\emptyset$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $M(\alpha)\subseteq M(\beta),\text{ }\alpha\models\beta$
\end_inset


\end_layout

